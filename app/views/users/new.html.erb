<h1>Create new user<h1>

<%= form_with scope: :user, url: users_path, local: true do |f|%>

  <p>
    <%= f.label :username %> <br/>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :email %> <br/>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password %> <br/>
    <%= f.text_field :password %>
  </p>

   <p>
    <%= f.submit %>
  </p>

<% end %>